<!DOCTYPE html>
<html>
<head>
  <title>Dognate</title>
  <%= stylesheet_link_tag    "members", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "jquery", "membersajax", "//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js", "members1"  %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if alert %>
  <p class="alert alert-error"><%= alert %></p>
  <p class="alert alert-error"><%#= error %></p>

<% end %>
<div id="navbar" class="navbar navbar-default navbar-fixed-top">
	<%= yield :navbar %>
</div>
<div id="container" data-spy="scroll" data-target="#navbar">
	<section id="intro" data-type="background" data-speed="20">
		<div id="middle" class="row-fluid">
			<%= yield :privacy %>
			<%= yield :intro %>
		</div>
	</section>
	<section id="home" data-type="background" data-speed="8">
		<div id="form">
			<%= yield :index %>
		</div>
	</section>
	<section id="about" data-type="background" data-speed="20">
		<%= yield :carousel %>
		<div id="about_article">
			<%= yield :supply_table %>
		</div>
	</section>
	<section id="ender" data-type="background" data-speed="8">
		<div id="ender_article">
			<%= yield :ender %>
		</div>
	</section>
	<section class="" id="footer" data-type="background" data-speed="100">
		<div class="" id="foot_article">
			<%= yield :footer %>
		</div>	
	</section>

</div>
<%= yield :supply_modal %>
<%= yield :wish_modal %>
</body>
</html>
